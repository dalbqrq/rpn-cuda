<?xml version="1.0" encoding="UTF-8"?>
<project basedir=".." name="RPn_Java">
    <property file="nbproject/nbjdk.properties"/>
    <property location="${netbeans.user}/build.properties" name="user.properties.file"/>
    <property file="${user.properties.file}"/>
    <import file="jdk.xml"/>
    <target depends="-jdk-init" name="all">
        <ant antfile="../../src/build.xml" inheritall="false" target="all"/>
    </target>
    <target depends="-jdk-init" name="clean">
        <ant antfile="../../src/build.xml" inheritall="false" target="clean"/>
    </target>
    <target depends="-jdk-init" name="doc">
        <ant antfile="../../src/build.xml" inheritall="false" target="doc"/>
    </target>
    <target depends="-jdk-init" name="run">
        <ant antfile="../../src/build.xml" inheritall="false" target="run"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-nb">
        <ant antfile="nbproject/ide-targets.xml" inheritall="false" target="debug-nb"/>
    </target>
    <target depends="-jdk-init" name="run-selected-file-in-src">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-file-in-src"/>
    </target>
    <target depends="-jdk-init" name="compile-selected-files-in-src">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="compile-selected-files-in-src"/>
    </target>
</project>
