#!/bin/bash

PROGDIR=$(dirname $(readlink -f $0))
CURRDIR=`pwd`
RPNDIR=$(dirname $PROGDIR)

if [ "`echo $1 | cut -c1`" == "/" ]; then
    IN=$1
else
    IN=$CURRDIR/$1
fi
if [ "`echo $2 | cut -c1`" == "/" ]; then
    OUT=$2
else
    OUT=$CURRDIR/$2
fi

cd $PROGDIR
[ -f ../src/make/env ] && . ../src/make/env

echo $JAVA_HOME/bin/java -DRPHOSTTYPE=$RPHOSTTYPE -Djava.library.path=$RPNDIR/lib/$RPHOSTTYPE -Drpnhome=$RPNDIR -jar $RPNDIR/lib/rpnbatch.jar $IN $OUT
$JAVA_HOME/bin/java -DRPHOSTTYPE=$RPHOSTTYPE -Djava.library.path=$RPNDIR/lib/$RPHOSTTYPE -Drpnhome=$RPNDIR -jar $RPNDIR/lib/rpnbatch.jar $IN $OUT
