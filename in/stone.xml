<?xml version="1.0"?>
<!DOCTYPE rpnconfiguration >
<rpnconfiguration>
    <PHYSICS name="Stone">


       <PHYSICSCONFIG name="fluxfunction">
        <PHYSICSPARAM name="grw" position="0" value ="1.0" />
        <PHYSICSPARAM name="grg" position="1" value ="1.0" />
        <PHYSICSPARAM name="gro" position="2" value ="1.0" />
        <PHYSICSPARAM name="muw" position="3" value ="1.0" />
        <PHYSICSPARAM name="mug" position="4" value ="1.0" />
        <PHYSICSPARAM name="muo" position="5" value ="1.0" />
        <PHYSICSPARAM name="vel" position="6" value ="1.0" />


    </PHYSICSCONFIG> 


    </PHYSICS>


    <METHOD name="Newton">
        <METHODPARAM  name="tolerance" value="0.001" />
    </METHOD>


    <CURVE name="hugoniotcurve">
        <CURVEPARAM name="resolution" value="128 128"/>
    </CURVE>


    <CURVE name="inflectioncurve">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="resolution" value="128 128"/>
    </CURVE>


    <CURVE name="levelcurve">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="resolution" value="128 128"/>
        <CURVEPARAM name="levels" value="0.1 0.2 0.5"/>

    </CURVE>


    <CURVE name="doublecontactcurve">
         <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="resolution" value="30 30"/>
    </CURVE>


    <CURVE name="hysteresiscurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>
        <CURVEPARAM name="resolution" value="30 30"/>
    </CURVE>


    <CURVE name="bifurcationcurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="caracteristicwhere" value="1"/>
        <CURVEPARAM name="singular" value="1"/>


    </CURVE>

    <CURVE name="boundaryextensioncurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>
        <CURVEPARAM name="edge" value="0"/>
        <CURVEPARAM name="edgeresolution" value="30"/>
        <CURVEPARAM name="resolution" value="60 60"/>


    </CURVE>

    <CURVE name="rarefactionextension">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristic" value="1"/>
        <CURVEPARAM name="resolution" value="60 60"/>
    </CURVE>

    <CURVE name="orbit">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="level" value="0.5"/>

    </CURVE>

    <VIEWCONFIGURATION modeldomain="2">

        <PROJDESC name="Axis0 Axis1">
            <VIEWPARAM name="axis" value="0 1"/>
            <VIEWPARAM name="vpwidth" value="350"/>
            <VIEWPARAM name="vpheight" value="350"/>
            <VIEWPARAM name="iso2equi" value="true"/>
        </PROJDESC>

    </VIEWCONFIGURATION>




</rpnconfiguration>
