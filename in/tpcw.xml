<?xml version="1.0"?>
<!DOCTYPE rpnconfiguration >
<RPNCONFIGURATION>

    <PHYSICS name="TPCW">
        
        <PHYSICSCONFIG name="fluxfunction">
            <PHYSICSPARAM name="grw" position="0" value ="3e-12" />
            <PHYSICSPARAM name="grg" position="1" value ="0.0" />
            <PHYSICSPARAM name="gro" position="2" value ="0.0" />
            <PHYSICSPARAM name="muw" position="3" value ="1.0" />
            <PHYSICSPARAM name="mug" position="4" value ="0.0" />
            <PHYSICSPARAM name="muo" position="5" value ="0.0" />
            <PHYSICSPARAM name="vel" position="6" value ="2.0" />
            <PHYSICSPARAM name="vel2" position="7" value ="2.0" />
            
            <PHYSICSPARAM name="T Typical" position ="8" value="304.63"/>
            <PHYSICSPARAM name="Rho Typical" position ="9" value="998.2"/>
            <PHYSICSPARAM name="U Typical" position="10" value="4.22e-3"/>
            <PHYSICSPARAM name="spline path" position= "11" value="/home/edsonlan/Java/rpn"/>
      
            
            <PHYSICSPARAM name="phi" position="12" value="0.38"/>

        </PHYSICSCONFIG>
        
       
     


    </PHYSICS>



    <METHOD name="Contour">
        <METHODPARAM  name="x-resolution" value="30" />
        <METHODPARAM  name="y-resolution" value="30" />

    </METHOD>


    <METHOD name="Newton">
        <METHODPARAM  name="tolerance" value="0.001" />
    </METHOD>

  <CURVECONFIGURATION name="doublecontactcurve">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="resolution" value="30 30"/>
    </CURVECONFIGURATION>

 <CURVECONFIGURATION name="inflectioncurve">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="resolution" value="128 128"/>
    </CURVECONFIGURATION>

    <CURVECONFIGURATION name="bifurcationcurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="0"/>
        <CURVEPARAM name="singular" value="1"/>
    </CURVECONFIGURATION>

    <CURVECONFIGURATION name="extensioncurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>
        <CURVEPARAM name="edge" value="0"/>
        <CURVEPARAM name="edgeresolution" value="128"/>
        <CURVEPARAM name="resolution" value="128 128"/>
        <CURVEPARAM name="characteristicwhere" value="0"/>
    </CURVECONFIGURATION>

    <CURVECONFIGURATION name="rarefactionextension">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="characteristic" value="1"/>
        <CURVEPARAM name="resolution" value="60 60"/>
    </CURVECONFIGURATION>

    <CURVECONFIGURATION name="wavecurve">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>

    </CURVECONFIGURATION>


    <CURVECONFIGURATION name="shock">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="xzero" value="3.4 0.5"/>
        <CURVEPARAM name="sigma" value="0.03"/>
    </CURVECONFIGURATION>





    <VIEWCONFIGURATION modeldomain="3">

        <PROJDESC name="Axis1 Axis0">
            <VIEWPARAM name="axis" value="1 0"/>
            <VIEWPARAM name="vpwidth" value="800"/>
            <VIEWPARAM name="vpheight" value="800"/>
            <VIEWPARAM name="iso2equi" value="false"/>

        </PROJDESC>


        <PROJDESC name="Axis2 Axis0">
            <VIEWPARAM name="axis" value="2 0"/>
            <VIEWPARAM name="vpwidth" value="800"/>
            <VIEWPARAM name="vpheight" value="800"/>
            <VIEWPARAM name="iso2equi" value="false"/>

        </PROJDESC>

        <!--
 <PROJDESC name="Axis1 Axis2">
        <VIEWPARAM name="axis" value="1 2"/>
        <VIEWPARAM name="vpwidth" value="600"/>
        <VIEWPARAM name="vpheight" value="650"/>
        <VIEWPARAM name="iso2equi" value="false"/>

    </PROJDESC>
  -->




    </VIEWCONFIGURATION>



</RPNCONFIGURATION>
