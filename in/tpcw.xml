<?xml version="1.0"?>
<!DOCTYPE rpnconfiguration >
<rpnconfiguration>
<PHYSICS name="TPCW">

<!--<BOUNDARY name="rect">

        <BOUNDARYPARAM name="dimension" value="3" />
        <BOUNDARYPARAM name="iso2equi" value="false" />
        <BOUNDARYPARAM name="limits" value="-0.5 0.5 -0.5 0.5 -0.5 0.5" />


    </BOUNDARY>-->

</PHYSICS>



     <METHOD name="Contour">
        <METHODPARAM  name="x-resolution" value="30" />
        <METHODPARAM  name="y-resolution" value="30" />

    </METHOD>


    <METHOD name="Newton">
        <METHODPARAM  name="tolerance" value="0.001" />
    </METHOD>



    <CURVE name="bifurcation">
        <CURVEPARAM name="leftfamily" value="0"/>
        <CURVEPARAM name="rightfamily" value="0"/>
        <CURVEPARAM name="caracteristicdomain" value="0"/>
        <CURVEPARAM name="singular" value="1"/>


    </CURVE>

    <CURVE name="boundaryextensioncurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicdomain" value="1"/>
        <CURVEPARAM name="x-domainpoints" value="101"/>
        <CURVEPARAM name="y-domainpoints" value="101"/>
        <CURVEPARAM name="edge" value="0"/>

    </CURVE>


    <CURVE name="rarefactionextension">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicdomain" value="1"/>
        <CURVEPARAM name="x-domainpoints" value="101"/>
        <CURVEPARAM name="y-domainpoints" value="101"/>
    </CURVE>


    <CURVE name="shock">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="xzero" value="3.4 0.5"/>
        <CURVEPARAM name="sigma" value="0.03"/>
    </CURVE>

 



<VIEWCONFIGURATION modeldomain="3">

    <PROJDESC name="Axis0 Axis1">
        <VIEWPARAM name="axis" value="1 0"/>
        <VIEWPARAM name="vpwidth" value="400"/>
        <VIEWPARAM name="vpheight" value="400"/>
        <VIEWPARAM name="iso2equi" value="false"/>

    </PROJDESC>


  <PROJDESC name="Axis0 Axis2">
        <VIEWPARAM name="axis" value="2 0"/>
        <VIEWPARAM name="vpwidth" value="400"/>
        <VIEWPARAM name="vpheight" value="400"/>
        <VIEWPARAM name="iso2equi" value="false"/>

    </PROJDESC>
  
