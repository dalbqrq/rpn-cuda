<?xml version="1.0"?>
<!DOCTYPE rpnconfiguration >
<rpnconfiguration>
    <PHYSICS name="TPCW">


        <ACCUMFUNCTION>
            <ACCUMPARAMS name="porosity" position="0" value="0.15"/>
        </ACCUMFUNCTION>

        <PHYSICSCONFIG name="thermodynamics">
            <PHYSICSPARAM name="absolut permeability" value="1.15"/>
            <PHYSICSPARAM name="viscosity" value="2.15"/>
            <PHYSICSPARAM name="spline path" value="/home/edsonlan/Java/rpn"/>
        </PHYSICSCONFIG>


    </PHYSICS>



    <METHOD name="Contour">
        <METHODPARAM  name="x-resolution" value="30" />
        <METHODPARAM  name="y-resolution" value="30" />

    </METHOD>


    <METHOD name="Newton">
        <METHODPARAM  name="tolerance" value="0.001" />
    </METHOD>



    <CURVE name="bifurcationcurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="0"/>
        <CURVEPARAM name="singular" value="1"/>


    </CURVE>

    <CURVE name="extensioncurve">
        <CURVEPARAM name="curvefamily" value="1"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>
        <CURVEPARAM name="edge" value="0"/>
        <CURVEPARAM name="edgeresolution" value="30"/>


    </CURVE>


    <CURVE name="wavecurve">
        <CURVEPARAM name="curvefamily" value="0"/>
        <CURVEPARAM name="domainfamily" value="1"/>
        <CURVEPARAM name="characteristicwhere" value="1"/>

    </CURVE>


    <CURVE name="shock">
        <CURVEPARAM name="family" value="0"/>
        <CURVEPARAM name="xzero" value="3.4 0.5"/>
        <CURVEPARAM name="sigma" value="0.03"/>
    </CURVE>





    <VIEWCONFIGURATION modeldomain="3">

        <PROJDESC name="Axis1 Axis0">
            <VIEWPARAM name="axis" value="1 0"/>
            <VIEWPARAM name="vpwidth" value="800"/>
            <VIEWPARAM name="vpheight" value="800"/>
            <VIEWPARAM name="iso2equi" value="false"/>

        </PROJDESC>


        <PROJDESC name="Axis2 Axis0">
            <VIEWPARAM name="axis" value="2 0"/>
            <VIEWPARAM name="vpwidth" value="800"/>
            <VIEWPARAM name="vpheight" value="800"/>
            <VIEWPARAM name="iso2equi" value="false"/>

        </PROJDESC>

        <!--
 <PROJDESC name="Axis1 Axis2">
        <VIEWPARAM name="axis" value="1 2"/>
        <VIEWPARAM name="vpwidth" value="600"/>
        <VIEWPARAM name="vpheight" value="650"/>
        <VIEWPARAM name="iso2equi" value="false"/>

    </PROJDESC>
  -->




    </VIEWCONFIGURATION>


</rpnconfiguration>
